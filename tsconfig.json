{
	"compileOnSave": false,
	"compilerOptions": {
		"declaration": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"module": "es2015",
		"moduleResolution": "node",
		"target": "es5",
		"sourceMap": true,
		"skipDefaultLibCheck": true,
		"strict": true,
		"lib": [
			"es2018",
			"dom"
		],
		"types": [ "webpack-env" ]
	}
}
